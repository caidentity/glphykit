{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true,
      "preview": true
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "copyFiles": [
          {
            "from": "../public/icons/**/*",
            "to": "public/icons"
          }
        ]
      }
    }
  ]
} 