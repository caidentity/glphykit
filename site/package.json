{
  "name": "Glyphkit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate-metadata && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "clean": "rm -rf .next node_modules",
    "clean:metadata": "rm -rf ./public/icon-metadata.json",
    "clean:public": "rm -rf ./public/icons",
    "prepare:public": "mkdir -p ./public/icons && cp -r ../public/icons/* ./public/icons/ 2>/dev/null || true",
    "generate-metadata": "set NODE_ENV=production&& tsx src/scripts/generateIconMetadata.ts",
    "prebuild": "npm run clean:metadata && npm run clean:public && npm run prepare:public && npm run generate:icons",
    "check:icons": "ls -la ../public/icons",
    "postbuild": "node scripts/verify-build.js",
    "generate:icons": "tsx src/scripts/generateIconRegistry.ts"
  },
  "dependencies": {
    "@glyphkit/glyphkit": "^0.0.109",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slider": "^1.2.2",
    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-virtual": "^3.11.2",
    "@types/next": "^8.0.7",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.469.0",
    "next": "^14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "scheduler": "^0.25.0",
    "styled-components": "^6.1.13",
    "stylis": "^4.3.4",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "sass": "1.69.7",
    "tailwindcss": "3.4.0",
    "tsx": "^4.19.2",
    "typescript": "5.3.3"
  }
}
