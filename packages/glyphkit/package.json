{
  "name": "@glyphkit/glyphkit",
  "version": "0.0.104",
  "description": "A customizable SVG icon toolkit for React applications",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run generate:icons && npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c rollup.config.mjs",
    "dev": "vite",
    "build:demo": "vite build",
    "preview": "vite preview",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "generate:icons": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node --project ./scripts/tsconfig.json scripts/generateIcons.ts"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.19.68",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^5.0.0"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "toolkit"
  ],
  "author": "caideneity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caidentity/glyphkit.git"
  },
  "bugs": {
    "url": "https://github.com/caidentity/glyphkit/issues"
  },
  "homepage": "https://github.com/caidentity/glyphkit#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
