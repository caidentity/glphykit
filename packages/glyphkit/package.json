{
  "name": "@glyphkit/glyphkit",
  "version": "0.0.105",
  "description": "A customizable SVG icon toolkit for React applications",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --project tsconfig.json --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c rollup.config.mjs",
    "clean": "rm -rf dist/ .next/ node_modules/.cache tsconfig.tsbuildinfo",
    "clean:all": "npm run clean && rm -rf node_modules/ && npm install",
    "generate:icons": "tsx scripts/generateIcons.ts",
    "predev": "npm run generate:icons",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "prepublishOnly": "npm run build && npm run test"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.19.68",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "jest-environment-jsdom": "^29.0.0"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "toolkit"
  ],
  "author": "caideneity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caidentity/glyphkit.git"
  },
  "bugs": {
    "url": "https://github.com/caidentity/glyphkit/issues"
  },
  "homepage": "https://github.com/caidentity/glyphkit#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false
}
